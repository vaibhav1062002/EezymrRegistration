        <!--start header -->
        <header>

            <div class="topbar d-flex align-items-center">
                <nav class="navbar navbar-expand">
                    <div class="mobile-toggle-menu">
                        <i class='bx bx-menu'></i>
                    </div>
                    <div class="search-bar flex-grow-1">
                        <div class="position-relative search-bar-box">
                            <input type="text" class="form-control search-control" placeholder="Type to search..."> <span class="position-absolute top-50 search-show translate-middle-y"><i class='bx bx-search'></i></span>
                            <span class="position-absolute top-50 search-close translate-middle-y"><i class='bx bx-x'></i></span>
                        </div>
                    </div>
            </div>

            <div class="page-wrapper">
                <div class="page-content">
                    <!--breadcrumb-->
                    <div class="page-breadcrumb d-none d-sm-flex align-items-center mb-3">
                        <div class="breadcrumb-title pe-3">Tables</div>


                        <div class="ps-3">
                            <nav aria-label="breadcrumb">
                                <ol class="breadcrumb mb-0 p-0">

                                    <li class="breadcrumb-item">
                                        <a href="javascript:;"><i class="bx bx-home-alt"></i></a>
                                    </li>
                                    <li class="breadcrumb-item active" aria-current="page">Data Table</li>
                                </ol>


                            </nav>

                        </div>

                    </div>


                    <div class="card">
                        <div class="card-body">
                            <div class="table-responsive">
                                <h6 class="mb-0 text-uppercase">Tooltips</h6>
                                <hr />
                                <div class="p-4 border rounded">

                                    <form class="row g-3" action="/Dashboard/AddPackageDetails" method="post">

                                        <!-- DoctorName -->

                                        <div class="col-md-4 position-relative">
                                            <label for="DoctorName" class="form-label">Doctor Name</label>
                                            <input type="text" class="form-control" asp-for="DoctorName" name="DoctorName" value="@Model.eazymrPackage[0].DoctorName">
                                            <span asp-validation-for="DoctorName" class="text-danger"></span>
                                        </div>-->

                                        <!-- Degree -->
                                        <div class="col-md-4 position-relative">
                                            <label for="Degree" class="form-label">Degree</label>
                                            <input type="text" class="form-control" asp-for="Degree" name="Degree" value="@Model.eazymrPackage[0].Degree">
                                            <span asp-validation-for="Degree" class="text-danger"></span>
                                        </div>

                                        <!--Specialization -->
                                        <div class="col-md-4 position-relative">
                                            <label for="Specialization" class="form-label">Specialization</label>
                                            <input type="text" class="form-control" asp-for="Specialization" name="Specialization" value="@Model.eazymrPackage[0].Specialization">
                                            <span asp-validation-for="Specialization" class="text-danger"></span>
                                        </div>

                                        <!-- RegistrationNumber -->
                                        <div class="col-md-4 position-relative">
                                            <label for="RegistrationNumber" class="form-label">Registration Number</label>
                                            <input type="text" class="form-control" asp-for="RegistrationNumber" name="RegistrationNumber" value="@Model.eazymrPackage[0].RegistrationNumber">
                                            <span asp-validation-for="RegistrationNumber" class="text-danger"></span>
                                        </div>

                                        <!-- Email -->
                                        <div class="col-md-4 position-relative">
                                            <label for="Email" class="form-label">Email</label>
                                            <input type="email" class="form-control" asp-for="Email" name="Email" value="@Model.eazymrPackage[0].Email">
                                            <span asp-validation-for="Email" class="text-danger"></span>
                                        </div>

                                        <!-- Continue similar blocks for other properties -->
                                        <!-- PackagePrice -->
                                        <div class="col-md-4 position-relative">
                                            <label for="PackagePrice" class="form-label">Package Price</label>
                                            <input type="text" class="form-control" asp-for="PackagePrice" name="PackagePrice" value="@Model.slectedPackage[1].PackagePrice">
                                            <span asp-validation-for="PackagePrice" class="text-danger"></span>
                                        </div>
                                </div>

                                <!-- Image -->
                                <div class="col-md-4 position-relative">
                                    <label for="Image" class="form-label">Image</label>
                                    <input type="text" class="form-control" asp-for="Image" name="Image">
                                    <span asp-validation-for="Image" class="text-danger"></span>
                                </div>

                                <!-- NumberOfDoctor -->
                                <div class="col-md-4 position-relative">
                                    <label for="NumberOfDoctor" class="form-label">Number of Doctors</label>
                                    <input type="text" class="form-control" asp-for="NumberOfDoctor">
                                    <span asp-validation-for="NumberOfDoctor" class="text-danger"></span>
                                </div>

                                <!-- Fertility -->
                                <div class="col-md-4 position-relative">
                                    <label for="Fertility" class="form-label">Fertility</label>
                                    <input type="text" class="form-control" asp-for="Fertility">
                                    <span asp-validation-for="Fertility" class="text-danger"></span>
                                </div>

                                <!-- Package -->
                                <div class="col-md-4 position-relative">
                                    <label for="Package" class="form-label">Package</label>
                                    <input type="text" class="form-control" asp-for="Package">
                                    <span asp-validation-for="Package" class="text-danger"></span>
                                </div>

                                <!-- ApplyCoupon -->
                                <div class="col-md-4 position-relative">
                                    <label for="ApplyCoupon" class="form-label">Apply Coupon</label>
                                    <input type="text" class="form-control" asp-for="ApplyCoupon">
                                    <span asp-validation-for="ApplyCoupon" class="text-danger"></span>
                                </div>

                                <!-- CouponNumber -->
                                <div class="col-md-4 position-relative">
                                    <label for="CouponNumber" class="form-label">Coupon Number</label>
                                    <input type="text" class="form-control" asp-for="CouponNumber">
                                    <span asp-validation-for="CouponNumber" class="text-danger"></span>
                                </div>

                                <!-- TAndC -->
                                <div class="col-md-4 position-relative">
                                    <label for="TAndC" class="form-label">Terms and Conditions</label>
                                    <input type="text" class="form-control" asp-for="TAndC">
                                    <span asp-validation-for="TAndC" class="text-danger"></span>
                                </div>

                                <!-- PackagePrice -->
                                <div class="col-md-4 position-relative">
                                    <label for="PackagePrice" class="form-label">Package Price</label>
                                    <input type="text" class="form-control" asp-for="PackagePrice">
                                    <span asp-validation-for="PackagePrice" class="text-danger"></span>
                                </div>
                            </div>

                            <div class="col-12">

                                <button type="submit" class="btn btn-primary">Add</button>
                            </div>

                            <div class="col-12">
                                <validation-summary></validation-summary>
                            </div>
                            </form>
                        </div>
                    </div>
                </div>

            </div>



            <!--end breadcrumb-->
            <h6 class="mb-0 text-uppercase">All Users Data</h6>
            <hr />
            <div class="card">
                <div class="card-body">
                    <div class="table-responsive">
                        <table id="example" class="table table-striped table-bordered">
                            <table id="example" class="table table-striped table-bordered">

                            <thead>
                                <tr>
                                    <th>ID</th>
                                    <th>Doctor Name</th>
                                    <th>Degree</th>
                                    <th>Specialization</th>
                                    <th>Registration Number</th>
                                    <th>Email</th>
                                    <th>Mobile Number</th>
                                    <th>Hospital Name</th>
                                    <th>Address</th>
                                    <th>State</th>
                                    <th>Pincode</th>
                                    <th>City</th>
                                    <th>Image</th>
                                    <th>Number of Doctors</th>
                                    <th>Fertility</th>
                                    <th>Package</th>
                                    <th>Apply Referral</th>
                                    <th>Referral Code </th>
                                    <th>Terms and Conditions</th>
                                    <th>Package Price</th>
                                </tr>
                            </thead>
                            <tbody>
                                @{
                                    int Counter = 1;
                                }

                                @foreach (var tuple in Model)
                                {
                                    var eazymrEntity = tuple.Item1;
                                    var selectPackageEntity = tuple.Item2;

                                    <tr>
                                        <td>@Counter</td>
                                        <td>@eazymrEntity.DoctorName</td>
                                        <td>@eazymrEntity.Degree</td>
                                        <td>@eazymrEntity.Specialization</td>
                                        <td>@eazymrEntity.RegistrationNumber</td>
                                        <td>@eazymrEntity.Email</td>
                                        <td>@eazymrEntity.MobileNumber</td>
                                        <td>@eazymrEntity.HospitalName</td>
                                        <td>@eazymrEntity.Address</td>
                                        <td>@eazymrEntity.State</td>
                                        <td>@eazymrEntity.Pincode</td>
                                        <td>@eazymrEntity.City</td>
                                        <td><img src="~/@eazymrEntity.Image" alt="Doctor Image" style="width: 80px;" /></td>
                                        <td>@selectPackageEntity.NumberOfDoctor</td>
                                        <td>@selectPackageEntity.Fertility</td>
                                        <td>@selectPackageEntity.Package</td>
                                        <td>@selectPackageEntity.ApplyCoupon</td>
                                        <td>@selectPackageEntity.CouponNumber</td>
                                        <td>@selectPackageEntity.TAndC</td>
                                        <td>@selectPackageEntity.PackagePrice</td>
                                        <td style="display:flex; width: fit-content;">
                                            <div>
                                                <form action="/Dashboard/AllDataEditPage" method="post">
                                                    <input type="text" id="Id" name="Id" value="@eazymrEntity.JoinTable" hidden />
                                                    <button class="btn btn-primary" type="submit">Edit</button>
                                                </form>
                                            </div>

                                            <div style="margin-left:6px">
                                                <form action="/Dashboard/DeleteFertility" method="post">
                                                    <input type="text" id="Id" name="Id" value="@eazymrEntity.JoinTable" hidden />
                                                    <button class="btn btn-outline-danger" type="submit">Delete</button>
                                                </form>
                                            </div>
                                        </td>
                                    </tr>

                                    Counter++;
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>

    </div>
    </div>


